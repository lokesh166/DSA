class RandomizedSet {
public:
    unordered_map<int, int> mp;
    vector<int> v;
    int size;
    
    RandomizedSet() {
        size = 0;
    }
    
    bool insert(int val) {
        // if val is not present 
        if(mp.find(val) == mp.end()) {
            mp[val] = size;
            v.push_back(val);
            size++;
            return true;
        }
        return false;
    }
    
    bool remove(int val) {
        // if val is present 
        if(mp.find(val) != mp.end()) {
            int idx = mp[val];
            v[idx] = v[size-1];
            v.pop_back();
            size--;
            mp[v[idx]] = idx;
            mp.erase(val);
            return true;
        }
        return false;
    }
    
    int getRandom() {
        int ans = rand()%size;
        return v[ans];
    }
};

/**
 * Your RandomizedSet object will be instantiated and called as such:
 * RandomizedSet* obj = new RandomizedSet();
 * bool param_1 = obj->insert(val);
 * bool param_2 = obj->remove(val);
 * int param_3 = obj->getRandom();
 */
